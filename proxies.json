{
    "$schema": "http://json.schemastore.org/proxies",
    "proxies": {
        "Logo": {
            "matchCondition": {
                "route": "/logo"
            },
            "backendUri": "https://%WEBSITE_HOSTNAME%/api/GetFunctionLogo"
        },
        "Pknw1": {
            "matchCondition": {
                "route": "/pknw1"
            },
            "backendUri": "https://www.pknw1.co.uk"
        },
        "Root": {
            "matchCondition": {
                "route": "/"
            },
            "backendUri": "https://www.pknw1.co.uk"
        }
    }
}
